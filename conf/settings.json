{
    // workspace: 
    "code-runner.runInTerminal": true,
    "files.associations": {
        "*.td": "arrow"
    },
    "editor.fontLigatures": false,
    "editor.fontFamily": "Monaco",
    "editor.fontSize": 15,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.formatOnSave": true,
    "editor.renderIndentGuides": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.bracketPairColorization.enabled": true,
    "explorer.copyRelativePathSeparator": "auto",
    "files.insertFinalNewline": true,
    "json.schemaDownload.enable": false,
    // clang-format: 
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: LLVM,ColumnLimit: 120, AlwaysBreakTemplateDeclarations: true, AlignAfterOpenBracket: AlwaysBreak, AlignTrailingComments: true, ReflowComments: true}",
    "C_Cpp.intelliSenseEngine": "Default",
    "C_Cpp.dimInactiveRegions": true,
    "C_Cpp.errorSquiggles": "Enabled",
    // others: 
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "material-icon-theme",
